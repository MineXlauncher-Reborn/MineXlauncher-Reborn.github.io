{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "Node",
        "outDir": "./",
        "strict": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noEmitOnError": false
    },
    "include": [
        "./**/*.ts"
    ]
}